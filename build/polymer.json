{
  "entrypoint": "index.html",
  "shell": "src/index.js",
  "fragments": [],
  "sources": [
    "images/**/*",
    "index.html",
    "src/index.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "src/**",
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "push-manifest.json"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "js": {
        "compile": "es5",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    }
  ],
  "npm": false,
  "moduleResolution": "node"
}